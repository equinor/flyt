{
  "name": "flyt",
  "radix-name": "vsm",
  "version": "0.2.34",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --watch",
    "release": "standard-version",
    "fetch-tags": "git fetch origin --tags --force",
    "release-dev": "yarn fetch-tags &&  git tag -f DEV   && yarn push-tags",
    "release-test": "yarn fetch-tags && git tag -f TEST  && yarn push-tags",
    "release-qa": "yarn fetch-tags &&   git tag -f QA    && yarn push-tags",
    "release-prod": "yarn fetch-tags && git tag -f PROD  && yarn push-tags",
    "push-tags": "git push origin --tags -f && echo Check status: https://console.radix.equinor.com/applications/vsm/jobs",
    "use-dev": "cp environment-variables/DEV.env .env && yarn dev",
    "use-dev-graph-poc": "cp environment-variables/DEV-GRAPH-POC.env .env && yarn dev",
    "use-qa-graph-poc": "cp environment-variables/QA-GRAPH-POC.env .env && yarn dev",
    "use-test": "cp environment-variables/TEST.env .env && yarn dev",
    "use-qa": "cp environment-variables/QA.env .env && yarn dev",
    "use-prod": "cp environment-variables/PROD.env .env && yarn dev",
    "run-dev": "docker build -t equinor-flyt . && docker run -p 3000:3000 --env-file ./environment-variables/DEV.env equinor-flyt",
    "run-dev-graph-poc": "docker build -t equinor-flyt . && docker run -p 3000:3000 --env-file ./environment-variables/DEV-GRAPH-POC.env equinor-flyt",
    "run-qa-graph-poc": "docker build -t equinor-flyt . && docker run -p 3000:3000 --env-file ./environment-variables/QA-GRAPH-POC.env equinor-flyt",
    "run-test": "docker build -t equinor-flyt . && docker run -p 3000:3000 --env-file ./environment-variables/TEST.env equinor-flyt",
    "run-qa": "docker build -t equinor-flyt . && docker run -p 3000:3000 --env-file ./environment-variables/QA.env equinor-flyt",
    "run-prod": "docker build -t equinor-flyt . && docker run -p 3000:3000 --env-file ./environment-variables/PROD.env equinor-flyt",
    "lint": "pretty-quick --staged && lint-staged",
    "rename": "sh scripts/changeName.sh",
    "genRadix": "sh scripts/generateRadixConfig.sh",
    "prepare": "husky install",
    "postinstall": "sh scripts/postInstall.sh",
    "genLockfile": "rm yarn.lock && rm -rf node_modules && yarn"
  },
  "dependencies": {
    "@azure/msal-browser": "2.38.0",
    "@azure/msal-react": "1.5.9",
    "@equinor/eds-core-react": "0.17.0",
    "@equinor/eds-icons": "0.10.0",
    "@types/jsonwebtoken": "9.0.2",
    "@types/node": "18.17.0",
    "@types/node-fetch": "2.6.4",
    "@types/react": "17.0.62",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@uiw/react-md-editor": "3.11.2",
    "axios": "^1.6.3",
    "easy-peasy": "5.2.0",
    "eslint-config-next": "^14.0.4",
    "jsonwebtoken": "9.0.1",
    "moment": "2.29.4",
    "next": "^14.0.4",
    "next-remove-imports": "1.0.11",
    "node-fetch": "2.6.12",
    "pixi-viewport": "4.38.0",
    "pixi.js": "6.2.2",
    "react": "^18.2.0",
    "react-device-detect": "2.2.3",
    "react-dom": "^18.2.0",
    "react-is": "17.0.2",
    "react-json-view": "1.21.3",
    "react-markdown": "8.0.4",
    "react-masonry-css": "1.0.16",
    "react-query": "3.39.3",
    "react-table": "7.8.0",
    "rehype-sanitize": "5.0.1",
    "remark-gfm": "3.0.1",
    "sass": "1.64.1",
    "sharp": "0.32.6",
    "socket.io": "4.6.1",
    "socket.io-client": "4.6.1",
    "styled-components": "5.3.3"
  },
  "devDependencies": {
    "@babel/core": "7.23.7",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "12.1.5",
    "babel-jest": "27.5.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.45.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-react": "1.1.7",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.33.0",
    "eslint-plugin-sonarjs": "0.11.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "6.12.0",
    "husky": "8.0.3",
    "jest": "27.5.1",
    "lint-staged": "13.2.3",
    "prettier": "2.8.8",
    "pretty-quick": "3.1.3",
    "standard-version": "9.5.0",
    "typescript": "4.5.5"
  },
  "resolutions": {
    "simple-get": "4.0.1"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --cache --fix"
    ],
    "*.js": "eslint --cache --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
